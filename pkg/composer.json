{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "bacon/bacon-qr-code": "^2.0",
        "barryvdh/laravel-dompdf": "^1.0",
        "chris-ware/nova-breadcrumbs": "^2.3",
        "fruitcake/laravel-cors": "^2.0",
        "gregoriohc/laravel-nova-theme-responsive": "^0.8.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "infinety-es/nova-filemanager": "^2.8",
        "laravel/framework": "^8.75",
        "laravel/horizon": "^5.8",
        "laravel/jetstream": "^2.6",
        "laravel/nova": "~3.0",
        "laravel/octane": "^1.2",
        "laravel/sanctum": "^2.11",
        "laravel/telescope": "^4.9",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.12",
        "mollie/laravel-mollie": "^2.0",
        "pdmfc/nova-action-button": "^0.7.4",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^2.11",
        "spiral/roadrunner": "^2.0",
        "titasgailius/search-relations": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.11",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
